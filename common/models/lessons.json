{
  "name": "Lessons",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "coins": {
      "type": "number",
      "required": true,
      "default": "0"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "curriculum": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "teachers": {
      "type": "belongsTo",
      "model": "Teachers",
      "foreignKey": ""
    },
    "enrollments": {
      "type": "hasMany",
      "model": "Enrollments",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
