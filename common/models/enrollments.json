{
  "name": "enrollments",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "startAt": {
      "type": "date",
      "required": true
    },
    "endAt": {
      "type": "date",
      "required": true
    },
    "status": {
      "type": "string",
      "required": true,
      "default": "open"
    },
    "coins": {
      "type": "number",
      "required": true,
      "default": "0"
    }
  },
  "validations": [],
  "relations": {
    "lessons": {
      "type": "belongsTo",
      "model": "lessons",
      "foreignKey": ""
    },
    "students": {
      "type": "belongsTo",
      "model": "students",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
