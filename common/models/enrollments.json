{
  "name": "Enrollments",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "startAt": {
      "type": "date",
      "required": true
    },
    "endAt": {
      "type": "date",
      "required": true
    },
    "status": {
      "type": "string",
      "required": true,
      "default": "open"
    },
    "coins": {
      "type": "number",
      "required": true,
      "default": "0"
    }
  },
  "validations": [],
  "relations": {
    "lessons": {
      "type": "belongsTo",
      "model": "Lessons",
      "foreignKey": ""
    },
    "students": {
      "type": "belongsTo",
      "model": "Students",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "student",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "student",
      "permission": "ALLOW",
      "property": "upsert"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "upsert"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "create"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "student",
      "permission": "ALLOW",
      "property": "reserve"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
